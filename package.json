{
  "name": "movies-hub",
  "version": "1.0.0",
  "description": "Your destination of search for movies",
  "main": "index.js",
  "babelOptions": {
    "compact": false
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --hot --env dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vishalini253/movie-hub.git"
  },
  "author": "Vishalini",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vishalini253/movie-hub/issues"
  },
  "homepage": "https://github.com/vishalini253/movie-hub#readme",
  "dependencies": {
    "file-loader": "^4.2.0",
    "history": "^4.10.1",
    "html-loader": "^0.5.5",
    "image-webpack-loader": "^6.0.0",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-slideshow-image": "^1.3.0",
    "url-loader": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
}
